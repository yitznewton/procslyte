{
    "name": "yitznewton/procslyte",
    "description": "ProCSLyte: a PHP parser for Citation Style Language (CSL)",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Yitzchak Schaffer",
            "email": "yitzchak.schaffer@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "igorw/get-in": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpmd/phpmd": "~2.0",
        "fabpot/php-cs-fixer": "dev-master",
        "squizlabs/php_codesniffer": "~1.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "Yitznewton\\Procslyte\\": "src/",
            "Yitznewton\\Tests\\Procslyte\\": "tests/"
        }
    }
}
