{
    "name": "yitznewton/procslyte",
    "description": "ProCSLyte: a PHP processor for Citation Style Language (CSL)",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Yitzchak Schaffer",
            "email": "yitzchak.schaffer@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "lstrojny/functional-php": "~1.0",
        "igorw/get-in": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpmd/phpmd": "~2.0",
        "fabpot/php-cs-fixer": "dev-master",
        "squizlabs/php_codesniffer": "~1.0",
